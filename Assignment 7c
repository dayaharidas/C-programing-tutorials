#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  int x1;
  int x2;
  int x3;
  printf("Enter 1st number:"),
  scanf("%d",&x1);
  printf("Enter 2nd number:");
  scanf("%d",&x2);
  printf("Enter 3rd number:");
  scanf("%d",&x3);
  if (x1> x2)
  {
    if (x1>x3)
    printf("x1 is greater");
    else
    printf("x2 is greater");
    
  }
  
  else if (x2>x3)
  printf("x2 is greater");
  else
  printf("x3 is greater");
  
  
  return 0;
}
