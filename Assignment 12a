#include <stdio.h>
int main()
{
  

int A[10][10],r,c,i,j;
printf("Enter the number of rows and columns of the matrix:");
scanf("%d%d",&r,&c);
for (i=0;i<r;i++)
{
  for(j=0;j<c;j++)
  {
    if (i=j)
    {
      A[i][j]=1;
      
    }
    else
    {
      
    
     A[i][j]=0;
     }
  }
}
printf("Identity matrix:\n");
for(i=0;i<r;i++)
{
  for(j=0;j<c;j++)
  {
    printf("%d\t",A[i][j]);
    
  } 
  printf("\n");
  
}
return 0;
}
